@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";
	ViewBag.Title = Model?.Value("pageTitle") ?? Model?.Name;
}


@if (!string.IsNullOrEmpty(ViewBag.Title)) 
{
	 @await Html.PartialAsync("partials/static/breadcrumb.cshtml")
}

<section class="projects-details">

	<div class="container">

		<section class="content">
			<div class="content-details">
				<div class="time">
					<i class="fa-regular fa-calendar-days"></i>
					<p>June 15, 2024</p>
				</div>
				@Model.Value("pageContent")

			<div class="project-tags">
				<p>tags:</p>
				<a href="#" class="tag">Business</a>
				<a href="#" class="tag">Consulting</a>
				<a href="#" class="tag">Onatrix</a>
			</div>

		</section>

		<aside>
			<div class="recent">
				<h3>Recent projects</h3>
				<div class="box">
					<img src="/Images/projects/grayImage.png" alt="">
					<div class="recent-content">
						<h5>Annual Report for Juva Inc.</h5>
						<div class="time">
							<i class="fa-regular fa-calendar-days"></i>
							<p>June 15, 2024</p>
						</div>
					</div>
				</div>
				<div class="box">
					<img src="/Images/projects/grayImage.png" alt="">
