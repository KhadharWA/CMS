@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContactPage>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "mainLayout.cshtml";
	ViewBag.Title = Model?.Value("pageTitle") ?? null;


	var freedDomBird = Model?.Value<IPublishedContent>("freedDomBird")?.Url() ?? "";
	var identity = Model?.Value<IPublishedContent>("identity")?.Url() ?? "";
	var natural = Model?.Value<IPublishedContent>("natural")?.Url() ?? "";
	var simpleaf = Model?.Value<IPublishedContent>("simpleaf")?.Url() ?? "";
	var globeSlogan = Model?.Value<IPublishedContent>("globeSlogan")?.Url() ?? "";
	var fossilGroup = Model?.Value<IPublishedContent>("fossilGroup")?.Url() ?? "";
}


@if (!string.IsNullOrEmpty(ViewBag.Title)) 
{
	 @await Html.PartialAsync("partials/static/breadcrumb.cshtml")
}


@await Html.PartialAsync("partials/static/contactInfo.cshtml")




<section class="callback">

                
	<div class="map-container">
		<iframe 
			src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3153.835434840104!2d144.95605431548144!3d-37.81362797975195!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6ad65d43f1d1f599%3A0x9eae12fd355a2976!2sMelbourne%20VIC%2C%20Australia!5e0!3m2!1sen!2sus!4v1631031156462!5m2!1sen!2sus" 
			width="100%" 
			height="748px" 
			style="border:0;" 
			allowfullscreen="" 
			loading="lazy">
		</iframe>
		<div class="overlay"></div>
	</div>
	
	<div class="container">
		<div class="form">
			<div class="title">
				<p>@Model.Value("callUsBackTitle")</p>
				<h2>@Model.Value("callUsBackHeadline")</h2>
			</div>
			<div class="sub-text">
                <p>@Model.Value("callUsBackText")</p>
            </div>
			<!-- Form Starts Here -->
			<form action="" method="post">
				<div class="input-container">
					@Html.GetBlockListHtml(Model.ContactForm)
				</div>
				
				

				
			</form>
		</div>
	</div>
</section>


<section class="partners">
	<div class="container">
		<img src="@freedDomBird" alt="">
		<img src="@identity" alt="">
		<img src="@natural" alt="">
		<img src="@simpleaf" alt="">
		<img src="@globeSlogan" alt="">
		<img src="@fossilGroup" alt="">
	</div>
</section>